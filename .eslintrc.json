{
	"extends": "next/core-web-vitals",
	"rules": {
		"semi": [
			"warn",
			"never"
		],
		"indent": [
			"warn",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"quotes": [
			"warn",
			"single"
		],
		"no-else-return": [
			"error",
			{
				"allowElseIf": false
			}
		],
		"no-shadow": [
			"error"
		],
		"react-hooks/exhaustive-deps": [
			0
		],
		"space-before-function-paren": [
			1
		]
	}
}